   "source": [
    "pred <- predict(train_svm, churn_test,\n",
    "                   decision.values = TRUE, probability = TRUE)\n",
    "prob <- attr(pred, \"probabilities\")[, 2]\n",
    "\n",
    "roc_curve <- roc(response = actual,\n",
    "                 predictor = prob)\n",
    "plot(roc_curve, print.auc = TRUE, auc.polygon=TRUE,\n",
    "     grid=c(0.2, 0.2), grid.col=c(\"green\", \"red\"),\n",
    "     max.auc.polygon=TRUE, auc.polygon.col=\"lightblue\",\n",
    "     print.thres=TRUE)"
   ]
