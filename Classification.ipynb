   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "k-Nearest Neighbors \n",
      "\n",
      "8000 samples\n",
      "  12 predictor\n",
      "   2 classes: 'X0', 'X1' \n",
      "\n",
      "No pre-processing\n",
      "Resampling: Cross-Validated (10 fold) \n",
      "Summary of sample sizes: 7200, 7199, 7200, 7200, 7199, 7200, ... \n",
      "Resampling results across tuning parameters:\n",
      "\n",
      "  k  ROC        Sens       Spec     \n",
      "  5  0.7755112  0.9422737  0.3692456\n",
      "  7  0.7896006  0.9529404  0.3513823\n",
      "  9  0.7967454  0.9587427  0.3397107\n",
      "\n",
      "ROC was used to select the optimal model using the largest value.\n",
      "The final value used for the model was k = 9.\n"
     ]
    }
   ],
   "source": [
    "churn_train$Exited <- as.factor(churn_train$Exited)\n",
    "levels(churn_train$Exited) <- make.names(levels(churn_train$Exited))\n",
    "\n",
    "ctrl <- trainControl(\n",
    "  method = \"cv\",\n",
    "  number = 10,\n",
    "  classProbs = TRUE, # classProbs = TRUE là quan trọng\n",
    "  summaryFunction = twoClassSummary\n",
    ")\n",
    "train_knn <- train(\n",
    "    Exited ~ ., # Công thức mô hình của bạn\n",
    "    data = churn_train, \n",
    "    method = \"knn\",\n",
    "    trControl = ctrl, \n",
    "    metric = \"ROC\", # Metric để tối ưu hóa k\n",
    ")\n",
    "print(train_knn)"
   ]
  },
